#include <iostream>
using namespace std;
int main() {
	// your code goes here
	int T;
	cin>>T;
	while(T--){
	    int X,Y;
	    cin>>X>>Y;
	    if(X>=Y){
	        cout<<0<<endl;
	    }
	    else if((Y-X)%8==0){
	        cout<<((Y-X)/8)<<endl;
	    }
	    else{
	        cout<<((Y-X)/8)+1<<endl;
	    }
	}
	return 0;
}
