#include<iostream>
#include<algorithm>
using namespace std;
int main(){
    int T;
    cin>>T;
    while(T--){
       int N,K,X;
       cin>>N;
	     int A[N];
	     for(int i=0;i<N;i++){
		        cin>>A[i];
	    }
	    cin>>K;
	    X=A[K-1];
	    sort(A,A+N);
	    for(int i=0;i<N;i++){
		       if(A[i]==X){
			         cout<<i+1<<endl;
		       }
	    }
    }
    return 0;
}
