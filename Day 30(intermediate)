#include <iostream>
using namespace std;
int main() {
	int T;
	cin>>T;
	while(T--){
       int A[3],B[3],count1=0,count2=0;
	     for(int i=0;i<3;i++){
		        cin>>A[i];
	     }
	     for(int i=0;i<3;i++){
		        cin>>B[i];
	     }
	     for(int i=0;i<3;i++){
		         if(A[i]>B[i]){
				         count1++;
			       }
			       else{
			 	          count2++;
    	       }
	    }
	    if(count1>count2){
		      cout<<"A"<<endl;
	   }
	   else{
		      cout<<"B"<<endl;
	   }
	}
	return 0;
}
