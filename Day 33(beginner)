#include <iostream>
using namespace std;
int main() {
    int T;
    cin>>T;
    while(T--){
        int A1,A2,B1,B2,x;
        cin>>A1>>A2>>B1>>B2;
        x=((A1-A2)+(B1-B2));
        if(x==0){
           cout<<"NO"<<endl;
        }
        else{
             if(x<0){
                cout<<"YES"<<endl;
             }
              else{
                cout<<"NO"<<endl;
             }
       }  
    }
	return 0;
}
