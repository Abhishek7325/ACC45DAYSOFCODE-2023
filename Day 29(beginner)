#include <iostream> 
using namespace std;
int main() {
    int T;
    cin>>T;
    while(T--){
		int W,X,Y,Z;
		cin>>W>>X>>Y>>Z;
		if(W==X||W==Y||W==Z){
			cout<<"YES"<<endl;
		}
		else{
			if(X+Y==W||Y+Z==W||Z+X==W||X+Y+Z==W){
				cout<<"YES"<<endl;
			}
			else{
				cout<<"NO"<<endl;
			}
		}
	}
	return 0;
}

