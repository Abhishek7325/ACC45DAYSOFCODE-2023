#include <iostream>
using namespace std;
int main() {
    int T,N,X;
    cin>>T;
    while(T--){
		int N,X;
		cin>>N>>X;
        int a[N],count=0;
        
        for(int i=0;i<N;i++){
             cin>>a[i];
		       if(a[i]>=X){
              count++;
           }
        }
        cout<<count<<endl;
    }    
	return 0;
}



